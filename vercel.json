{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.ts",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build/server"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}